<br>
<h1 style="text-align: center;">Telefones das Empresas </h1>
<br>

<table class="table">
  <thead>
    <tr>
      <th scope="col">No Empresa</th>
      <th scope="col">Telefone</th>
    </tr>
  </thead>
  <tbody id="companies_phonesBody">
		{{#each companies_phones}}
				<tr id="row:{{this.idEmpresa}}">
				<th scope="row">{{this.idEmpresa}}</th>
				<td>{{this.telefone}}</td>
			</tr>
		{{/each }}
  </tbody>
</table>